{
		"name": "express-typescript-starter",
		"version": "0.1.0",
		"description": "A starting point for Node.js express apps with TypeScript",
		"repository": {
				"type": "git",
				"url": "https://github.com/sahat/hackathon-starter.git"
		},
		"author": "Vo Lam Thien Phu",
		"license": "MIT",
		"scripts": {
				"start": "npm run serve:gcloud",
				"sequelize": "./node_modules/.bin/sequelize",
				"sequelize:migrate": "./node_modules/.bin/sequelize db:migrate",
				"deploy:gcloud": "npm run build && rm -rf ./dist && gcloud app deploy",
				"serve:gcloud": "npm run build-babel && node dist/index.js",
				"build": "npm run clear && npm run build-ts && npm run build-babel",
				"serve": "nodemon dist/server.js",
				"watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-babel\" \"npm run serve\"",
				"test": "jest --forceExit",
				"build-ts": "tsc",
				"watch-ts": "tsc -w",
				"build-webpack": "webpack",
				"watch-webpack": "webpack --watch",
				"build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
				"watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
				"tslint": "tslint -c tslint.json -p tsconfig.json",
				"tslint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
				"copy-static-assets": "node copyStaticAssets.js",
				"debug": "npm run build && npm run watch-debug",
				"serve-debug": "nodemon --inspect dist/server.js",
				"watch-debug": "npm run clear && concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-babel\"",
				"logs": "heroku logs --tail",
				"dev": "npm run build && nodemon dist/index.js",
				"build-babel": "npx babel build --out-dir dist --source-maps",
				"watch-babel": "./node_modules/.bin/babel-watch build/index.js",
				"watch-babel:prod": "NODE_ENV=production ./node_modules/.bin/babel-watch build/index.js",
				"watch-babel:prod:win": ".\\node_modules\\.bin\\babel-watch build\\index.js",
				"docker:mongo": "cd docker && docker-compose -f docker-compose.mongo.yml up -d",
				"docker:mongo:stop": "cd docker && docker-compose -f docker-compose.mongo.yml stop",
				"docker:pg": "cd docker && docker-compose -f docker-compose.postgresql.yml up -d",
				"docker:pg:stop": "cd docker && docker-compose -f docker-compose.postgresql.yml stop",
				"clear": "rm -rf ./build && rm -rf ./dist",
				"heroku:config:push": "heroku config:push -c -f .env -o",
				"w1": "tsc -w",
				"w2": "npx babel-watch build/index.js",
				"start:dev": "concurrently -k -p \"[{name}]\" -n \"WATCH,HTTP,OTHER\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run w1\" \"npm run w2\"",
				"db:migrate": "npx sequelize-cli db:migrate --name",
				"db:migration": "npx sequelize migration:create --name",
				"db:migrate:undo": "npx sequelize db:migrate:undo --name",
				"migrate:dev": "sequelize db:migrate --env development",
				"migrate:prod": "sequelize db:migrate --env production"
		},
		"jest": {
				"collectCoverage": false,
				"globals": {
						"__TS_CONFIG__": "tsconfig.json"
				},
				"moduleFileExtensions": [
						"ts",
						"js"
				],
				"transform": {
						"^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
				},
				"testMatch": [
						"**/test/**/*.test.(ts|js)"
				],
				"testEnvironment": "node",
				"moduleNameMapper": {
						"@/(.*)": "./src/$1"
				},
				"moduleDirectories": [
						"node_modules",
						"src"
				]
		},
		"devDependencies": {
				"@babel/cli": "^7.23.4",
				"@babel/core": "^7.23.7",
				"@babel/node": "^7.22.19",
				"@babel/plugin-syntax-import-source": "^7.23.7",
				"@babel/plugin-transform-runtime": "^7.23.7",
				"@babel/preset-env": "^7.23.8",
				"@babel/preset-typescript": "^7.23.3",
				"@commitlint/cli": "^18.4.4",
				"@types/async": "^3.2.24",
				"@types/bluebird": "^3.5.42",
				"@types/body-parser": "^1.19.5",
				"@types/bson": "^4.2.0",
				"@types/compression": "^1.7.5",
				"@types/cors": "^2.8.17",
				"@types/del": "^4.0.0",
				"@types/dotenv": "^2.0.20",
				"@types/errorhandler": "^1.5.3",
				"@types/express": "^4.17.21",
				"@types/express-serve-static-core": "^4.17.41",
				"@types/express-session": "^1.17.10",
				"@types/jest": "^29.5.11",
				"@types/jwt-simple": "^0.5.36",
				"@types/lodash": "^4.14.202",
				"@types/md5": "^2.3.5",
				"@types/mime-types": "^2.1.4",
				"@types/morgan": "^1.9.9",
				"@types/multer": "^1.4.11",
				"@types/node": "^20.10.7",
				"@types/sequelize": "^4.28.19",
				"@types/socket.io": "^3.0.2",
				"babel-loader": "^9.1.3",
				"babel-plugin-istanbul": "^6.1.1",
				"babel-plugin-root-import": "^6.6.0",
				"babel-polyfill": "^6.26.0",
				"babel-register": "^6.26.0",
				"babel-watch": "^7.8.1",
				"concurrently": "^8.2.2",
				"eslint": "^8.56.0",
				"husky": "^8.0.3",
				"jest": "^29.7.0",
				"lint-staged": "^15.2.0",
				"node-sass": "^9.0.0",
				"nodemon": "^3.0.2",
				"sequelize-cli": "^6.6.2",
				"ts-jest": "^29.1.1",
				"ts-loader": "^9.5.1",
				"ts-node": "^10.9.2",
				"tslint": "^6.1.3",
				"typescript": "^5.3.3",
				"webpack": "^5.89.0",
				"webpack-cli": "^5.1.4"
		},
		"dependencies": {
				"@babel/runtime": "^7.23.8",
				"@commitlint/config-conventional": "^18.4.4",
				"@sendgrid/mail": "^8.1.0",
				"@socket.io/redis-adapter": "^8.3.0",
				"@socket.io/redis-emitter": "^5.1.0",
				"@types/bcrypt": "^5.0.2",
				"@types/swagger-jsdoc": "^6.0.4",
				"@types/swagger-ui-express": "^4.1.6",
				"@typescript-eslint/eslint-plugin": "^6.18.1",
				"@typescript-eslint/parser": "^6.18.1",
				"ajv": "^8.12.0",
				"ajv-errors": "^3.0.0",
				"ajv-formats": "^2.1.1",
				"ajv-keywords": "^5.1.0",
				"apple-signin": "^1.0.9",
				"aws-sdk": "^2.1630.0",
				"axios": "^1.6.5",
				"bcrypt": "^5.1.1",
				"body-parser": "^1.20.2",
				"buffer-image-size": "^0.6.4",
				"cluster": "^0.7.7",
				"cors": "^2.8.5",
				"crypto": "^1.0.1",
				"dotenv": "^4.0.0",
				"express": "^4.18.2",
				"express-session": "^1.17.3",
				"express-typescript-starter": "file:",
				"fetch": "^1.1.0",
				"firebase-admin": "^12.0.0",
				"get-stream": "^8.0.1",
				"google-auth-library": "^9.10.0",
				"jwt-simple": "^0.5.6",
				"livekit-server-sdk": "^2.5.0",
				"lodash": "^4.17.21",
				"md5": "^2.3.0",
				"mime-types": "^2.1.35",
				"moment": "^2.30.1",
				"moment-timezone": "^0.5.44",
				"morgan": "^1.10.0",
				"multer": "^1.4.5-lts.1",
				"mysql2": "^3.7.0",
				"node-cron": "^3.0.3",
				"node-schedule": "^2.1.1",
				"nodemailer": "^6.9.13",
				"pg": "^8.11.3",
				"prettier": "^3.1.1",
				"querystring": "^0.2.1",
				"realkorean-api": "file:",
				"redis": "^4.6.14",
				"sequelize": "^6.35.2",
				"sharp": "^0.33.1",
				"socket.io": "^4.7.5",
				"swagger-jsdoc": "^6.2.8",
				"swagger-ui-express": "^5.0.0",
				"uuidv4": "^6.2.13"
		}
}
